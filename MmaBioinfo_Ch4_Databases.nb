  3.7207997072983637`*^9}},ExpressionUUID->"66b49fc5-05b0-4dc1-b8cc-\
1cc15e18f24b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ucscDASSequenceParser", "[", "input_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"str", "=", "input"}], ",", "stringsAll", ",", "info1", ",", 
      "dnaLength", ",", "sequenceInfo"}], "}"}], ",", 
    RowBox[{
     RowBox[{"stringsAll", "=", 
      RowBox[{"Association", "@", 
       RowBox[{"Flatten", "@", 
        RowBox[{"StringCases", "[", 
         RowBox[{"input", ",", 
          RowBox[{
           RowBox[{
           "\"\<<SEQUENCE \>\"", "~~", "x__", "~~", "\"\<>\>\"", "~~", "__", 
            "~~", "\"\<<DNA \>\"", "~~", "y__", "~~", "\"\<>\>\"", "~~", 
            RowBox[{"seq", ":", "__"}], "~~", "\"\<</DNA>\>\""}], 
           "\[RuleDelayed]", "  ", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", " ", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "&"}], "/@", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"StringSplit", "[", 
                  RowBox[{"#", ",", "\"\<=\>\""}], "]"}], "&"}], "/@", 
                RowBox[{"StringSplit", "[", "x", "]"}]}], ")"}]}], ",", " ", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", " ", 
                   RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "&"}], "@", 
                 RowBox[{"StringSplit", "[", 
                  RowBox[{"#", ",", "\"\<=\>\""}], "]"}]}], ")"}], "&"}], "@",
               "y"}], ",", 
             RowBox[{"\"\<sequence\>\"", "\[Rule]", 
              RowBox[{"StringJoin", "[", 
               RowBox[{"StringSplit", "[", "seq", "]"}], "]"}]}]}], "}"}]}]}],
          "]"}]}]}]}], ";", 
     RowBox[{"Return", "[", "stringsAll", "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.720477039783429*^9, 3.7204774009698887`*^9}, {
   3.7204774760718803`*^9, 3.7204774945595512`*^9}, {3.720477569000214*^9, 
   3.720477695569192*^9}, {3.7204783907196503`*^9, 3.72047845496095*^9}, {
   3.7204784924501143`*^9, 3.7204788767853403`*^9}, {3.720479776545803*^9, 
