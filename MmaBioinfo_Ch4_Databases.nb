Cell["Additionally, we could automate the import as well:", "Text",
 CellChangeTimes->{{3.720480685355837*^9, 
  3.720480699693465*^9}},ExpressionUUID->"d5261ca8-587e-45db-a9fb-\
c9f8e1bc2b73"],

Cell[BoxData[
 RowBox[{
  RowBox[{"UCSCSequence", "[", 
   RowBox[{"genomeVersion_", ",", "chromosome_", ",", "start_", ",", "end_"}],
    "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"gen", "=", "genomeVersion"}], ",", 
      RowBox[{"chr", "=", "chromosome"}], ",", 
      RowBox[{"st", "=", "start"}], ",", 
      RowBox[{"en", "=", "end"}], ",", "queryOut", ",", "stringsAll", ",", 
      "info1", ",", "dnaLength", ",", "sequenceInfo"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"queryOut", "=", 
      RowBox[{"Import", "[", 
       RowBox[{
        RowBox[{
        "\"\<http://genome.ucsc.edu/cgi-bin/das/\>\"", "<>", "gen", "<>", 
         "\"\</dna?segment=\>\"", "<>", "chr", "<>", "\"\<:\>\"", "<>", "st", 
         "<>", "\"\<,\>\"", "<>", "en"}], ",", "\"\<Text\>\""}], "]"}]}], ";", 
     RowBox[{"stringsAll", "=", 
      RowBox[{"Association", "@", 
       RowBox[{"Flatten", "@", 
        RowBox[{"StringCases", "[", 
         RowBox[{"queryOut", ",", 
          RowBox[{
           RowBox[{
           "\"\<<SEQUENCE \>\"", "~~", "x__", "~~", "\"\<>\>\"", "~~", "__", 
            "~~", "\"\<<DNA \>\"", "~~", "y__", "~~", "\"\<>\>\"", "~~", 
            RowBox[{"seq", ":", "__"}], "~~", "\"\<</DNA>\>\""}], 
           "\[RuleDelayed]", "  ", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", " ", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "&"}], "/@", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"StringSplit", "[", 
                  RowBox[{"#", ",", "\"\<=\>\""}], "]"}], "&"}], "/@", 
                RowBox[{"StringSplit", "[", "x", "]"}]}], ")"}]}], ",", " ", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", " ", 
                   RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "&"}], "@", 
                 RowBox[{"StringSplit", "[", 
                  RowBox[{"#", ",", "\"\<=\>\""}], "]"}]}], ")"}], "&"}], "@",
               "y"}], ",", 
             RowBox[{"\"\<sequence\>\"", "\[Rule]", 
              RowBox[{"StringJoin", "@", 
               RowBox[{"StringSplit", "@", "seq"}]}]}]}], "}"}]}]}], 
         "]"}]}]}]}], ";", 
     RowBox[{"Return", "[", "stringsAll", "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.720480707247981*^9, 3.7204809747423763`*^9}, 
   3.7204812382983837`*^9, {3.720799759749435*^9, 3.720799802038149*^9}, {
